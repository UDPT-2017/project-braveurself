<div class="row">
    <div class="row col-lg-6 col-md-6 col-sm-8 col-xs-12">
        <a href="javascript:void(0)" class="button">
            <h5 data-toggle="collapse" data-target="#HighestPrice">
                Giá cao nhất
                <div class="glyphicon glyphicon-menu-down"></div>
            </h5>
        </a>
        <div id="HighestPrice" class="collapse in carousel slide" data-ride="carousel">
            <!-- Indicators -->
            <ol class="carousel-indicators">
                <li data-target="#HighestPrice" data-slide-to="0" class="active"></li>
                <li data-target="#HighestPrice" data-slide-to="1"></li>
                <li data-target="#HighestPrice" data-slide-to="2"></li>
                <li data-target="#HighestPrice" data-slide-to="3"></li>
                <li data-target="#HighestPrice" data-slide-to="4"></li>
            </ol>

            <!-- Wrapper for slides -->
            <div class="carousel-inner" style=" width:100%; height: 100% !important;">
                <% products_HighestPrice.forEach(function(product, index){ %>
                    <% if (index == 0) { %>
                        <div class="item active">
                            <img src=<%=product.image1%> alt="Los Angeles" style="width:100%;">
                            <div class="carousel-caption">
                                <h5>Tên sản phẩm:
                                    <%=product.name%>
                                </h5>
                                <h5>Giá hiện tại:
                                    <%=product.curPrice%>
                                </h5>
                                <h5>Giá mua ngay:
                                    <%=product.immediatePrice%>
                                </h5>
                            </div>
                        </div>
                        <% } %>
                            <% if (index != 0) { %>
                                <div class="item">
                                    <img src=<%=product.image1%> alt="Los Angeles" style="width:100%;">
                                    <div class="carousel-caption">
                                        <h5>Tên sản phẩm:
                                            <%=product.name%>
                                        </h5>
                                        <h5>Giá hiện tại:
                                            <%=product.curPrice%>
                                        </h5>
                                        <h5>Giá mua ngay:
                                            <%=product.immediatePrice%>
                                        </h5>
                                    </div>
                                </div>
                                <% } %>

                                    <% }); %>


            </div>

            <!-- Left and right controls -->
            <a class="left carousel-control" href="#HighestPrice" data-slide="prev">
                <span class="glyphicon glyphicon-chevron-left"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="right carousel-control" href="#HighestPrice" data-slide="next">
                <span class="glyphicon glyphicon-chevron-right"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>
    </div>

    <div class="row col-lg-1 col-md-1 col-sm-0 col-xs-0">
    </div>

